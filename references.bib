@article{dingPolygenicScoringAccuracy2023,
  title = {Polygenic Scoring Accuracy Varies across the Genetic Ancestry Continuum},
  author = {Ding, Yi and Hou, Kangcheng and Xu, Ziqi and Pimplaskar, Aditya and Petter, Ella and Boulier, Kristin and Priv{\'e}, Florian and Vilhj{\'a}lmsson, Bjarni J. and Olde Loohuis, Loes M. and Pasaniuc, Bogdan},
  year = {2023},
