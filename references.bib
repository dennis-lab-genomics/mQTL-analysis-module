  shorttitle = {Gene-{{Environment Interplay}} in {{Common Complex Diseases}}},
  author = {Bookman, Ebony B. and McAllister, Kimberly and Gillanders, Elizabeth and Wanke, Kay and Balshaw, David and Rutter, Joni and Reedy, Jill and Shaughnessy, Daniel and {Agurs-Collins}, Tanya and Paltoo, Dina and Atienza, Audie and Bierut, Laura and Kraft, Peter and Fallin, M. Daniele and Perera, Frederica and Turkheimer, Eric and Boardman, Jason and Marazita, Mary L. and Rappaport, Stephen M. and Boerwinkle, Eric and Suomi, Stephen J. and Caporaso, Neil E. and {Hertz-Picciotto}, Irva and Jacobson, Kristen C. and Lowe, William L. and Goldman, Lynn R. and Duggal, Priya and Gunnar, Megan R. and Manolio, Teri A. and Green, Eric D. and Olster, Deborah H. and Birnbaum, Linda S.},
  year = {2011},
