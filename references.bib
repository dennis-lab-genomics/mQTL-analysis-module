@article{luoEstimatingHeritabilityIts2021,
  title = {Estimating Heritability and Its Enrichment in Tissue-Specific Gene Sets in Admixed Populations},
  author = {Luo, Yang and Li, Xinyi and Wang, Xin and Gazal, Steven and Mercader, Josep Maria and {23 and Me Research Team} and {SIGMA Type 2 Diabetes Consortium} and Neale, Benjamin M and Florez, Jose C and Auton, Adam and Price, Alkes L and Finucane, Hilary K and Raychaudhuri, Soumya},
