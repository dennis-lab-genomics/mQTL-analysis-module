@article{sunApplicationPartialLeast2019,
  title = {Application of Partial Least Squares in Exploring the Genome Selection Signatures between Populations},
  author = {Sun, Hao and Zhang, Zhe and Olasege, Babatunde Shittu and Xu, Zhong and Zhao, Qingbo and Ma, Peipei and Wang, Qishan and Pan, Yuchun},
  year = {2019},
