  doi = {10.1016/bs.pbr.2018.09.008},
  urldate = {2020-02-19},
  abstract = {The MAPK pathway is a prominent intracellular signaling pathway regulating various intracellular functions. Components of this pathway are mutated in a related collection of congenital syndromes collectively referred to as neuro-cardio-facio-cutaneous syndromes (NCFC) or Rasopathies. Recently, it has been appreciated that these disorders are associated with autism spectrum disorders (ASD). In addition, idiopathic ASD has also implicated the MAPK signaling cascade as a common pathway that is affected by many of the genetic variants that have been found to be linked to ASDs. This chapter describes the components of the MAPK pathway and how it is regulated. Furthermore, this chapter will highlight the various functions of the MAPK pathway during both embryonic development of the central nervous system (CNS) and its roles in neuronal physiology and ultimately, behavior. Finally, we will summarize the perturbations to MAPK signaling in various models of autism spectrum disorders and Rasopathies to highlight how dysregulation of this pivotal pathway may contribute to the pathogenesis of autism.},
  langid = {english},
  keywords = {Autism spectrum disorders,Corticogenesis,ERK,MAP kinase,Neural progenitors,Rasopathies},
  file = {/home/wcasazza/Dropbox/Zotero/storage/QKH7J3TR/Vithayathil et al_2018_Chapter 3 - ERK-MAPK signaling and autism spectrum disorders.pdf;/home/wcasazza/Dropbox/Zotero/storage/S36W5PLZ/S0079612318301146.html}
}

