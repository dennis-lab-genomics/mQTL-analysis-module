  author = {Harries, Lorna W. and Locke, Jonathan M. and Shields, Beverley and Hanley, Neil A. and Hanley, Karen Piper and Steele, Anna and Nj{\o}lstad, P{\aa}l R. and Ellard, Sian and Hattersley, Andrew T.},
  year = {2008},
  month = jun,
  journal = {Diabetes},
  volume = {57},
