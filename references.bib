@article{leeGenomeWideExpressionQuantitative2018,
  title = {Genome-{{Wide Expression Quantitative Trait Loci Analysis Using Mixed Models}}},
  author = {Lee, Chaeyoung},
  year = {2018},
  journal = {Frontiers in Genetics},
  volume = {9},
  issn = {1664-8021},
  doi = {10.3389/fgene.2018.00341},
  urldate = {2019-12-06},
  abstract = {Expression quantitative trait loci (eQTLs) are important for understanding the genetic basis of cellular activities and complex phenotypes. Genome-wide eQTL analyses can be effectively conducted by employing a mixed model. The mixed model includes random polygenic effects with variability, which can be estimated by the covariance structure of pairwise relationships among individuals based on genotype information for nucleotide sequence variants. This increases the accuracy of identifying eQTLs by avoiding population stratification. Its extensive use will accelerate our understanding of the genetics of gene expression and complex phenotypes. An overview of genome-wide eQTL analyses using mixed model methodology is provided, including discussions of both theoretical and practical issues. The advantages of employing mixed models are also discussed in this review.},
  langid = {english},
  keywords = {expression quantitative trait locus,Genetic association,genetic variance,heritability,Mixed model},
  file = {/home/wcasazza/Dropbox/Zotero/storage/LEP4EQ2P/Lee - 2018 - Genome-Wide Expression Quantitative Trait Loci Ana.pdf}
}

