  langid = {english},
  keywords = {Data integration,Genetics,Statistical methods},
  file = {/home/wcasazza/Dropbox/Zotero/storage/3B2PYQMY/Sadler et al. - 2022 - Quantifying the role of transcript levels in media.pdf}
}

