@article{internationalobsessivecompulsivedisorderfoundationgeneticscollaborativeiocdf-gcandocdcollaborativegeneticsassociationstudiesocgasRevealingComplexGenetic2018,
  title = {Revealing the Complex Genetic Architecture of Obsessive-Compulsive Disorder Using Meta-Analysis},
  author = {{International Obsessive Compulsive Disorder Foundation Genetics Collaborative (IOCDF-GC) and OCD Collaborative Genetics Association Studies (OCGAS)}},
  year = {2018},
