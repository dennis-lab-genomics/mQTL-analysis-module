}

@article{brownExpressionReflectsPopulation2018,
  title = {Expression Reflects Population Structure},
  author = {Brown, Brielin C. and Bray, Nicolas L. and Pachter, Lior},
  year = {2018},
  month = dec,
  journal = {PLOS Genetics},
  volume = {14},
  number = {12},
  pages = {e1007841},
  publisher = {{Public Library of Science}},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1007841},
  urldate = {2020-02-26},
  abstract = {Population structure in genotype data has been extensively studied, and is revealed by looking at the principal components of the genotype matrix. However, no similar analysis of population structure in gene expression data has been conducted, in part because a na\"ive principal components analysis of the gene expression matrix does not cluster by population. We identify a linear projection that reveals population structure in gene expression data. Our approach relies on the coupling of the principal components of genotype to the principal components of gene expression via canonical correlation analysis. Our method is able to determine the significance of the variance in the canonical correlation projection explained by each gene. We identify 3,571 significant genes, only 837 of which had been previously reported to have an associated eQTL in the GEUVADIS results. We show that our projections are not primarily driven by differences in allele frequency at known cis-eQTLs and that similar projections can be recovered using only several hundred randomly selected genes and SNPs. Finally, we present preliminary work on the consequences for eQTL analysis. We observe that using our projection co-ordinates as covariates results in the discovery of slightly fewer genes with eQTLs, but that these genes replicate in GTEx matched tissue at a slightly higher rate.},
  langid = {english},
  keywords = {Europe,Gene expression,Gene regulation,Molecular genetics,Permutation,Population genetics,Principal component analysis,Structural genomics},
  file = {/home/wcasazza/Dropbox/Zotero/storage/H78BARPE/Brown et al_2018_Expression reflects population structure.pdf;/home/wcasazza/Dropbox/Zotero/storage/VFCBV9VU/article.html}
}

