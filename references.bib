@article{Masliah2013-ob,
  title = {Distinctive Patterns of {{DNA}} Methylation Associated with {{Parkinson}} Disease: Identification of Concordant Epigenetic Changes in Brain and Peripheral Blood Leukocytes},
  author = {Masliah, Eliezer and Dumaop, Wilmar and Galasko, Douglas and Desplats, Paula},
