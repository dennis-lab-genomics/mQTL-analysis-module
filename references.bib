@article{sourenDNAMethylationSignatures2019,
  title = {{{DNA}} Methylation Signatures of Monozygotic Twins Clinically Discordant for Multiple Sclerosis},
  author = {Souren, Nicole Y. and Gerdes, Lisa A. and Lutsik, Pavlo and Gasparoni, Gilles and Beltr{\'a}n, Eduardo and Salhab, Abdulrahman and K{\"u}mpfel, Tania and Weichenhan, Dieter and Plass, Christoph and Hohlfeld, Reinhard and Walter, J{\"o}rn},
  year = {2019},
