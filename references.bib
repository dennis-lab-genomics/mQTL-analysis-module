  file = {/home/wcasazza/Dropbox/Zotero/storage/NBWFN67R/Kelley, Snoek, Rinn - 2016 - Basset learning the regulatory code of the accessible genome with deep convolutional neural networks(3).pdf}
}

@article{kentHumanGenomeBrowser2002,
