@article{pengGeneticRegulationPlacental2018,
  title = {Genetic Regulation of the Placental Transcriptome Underlies Birth Weight and Risk of Childhood Obesity},
  author = {Peng, Shouneng and Deyssenroth, Maya A. and Narzo, Antonio F. Di and Cheng, Haoxiang and Zhang, Zhongyang and Lambertini, Luca and Ruusalepp, Arno and Kovacic, Jason C. and Bjorkegren, Johan L. M. and Marsit, Carmen J. and Chen, Jia and Hao, Ke},
  year = {2018},
