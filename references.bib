  number = {1},
  pages = {180--186},
  publisher = {{Nature Publishing Group}},
  issn = {1061-4036},
  doi = {10.1038/s41588-018-0271-0},
  urldate = {2019-01-18},
  abstract = {Different exposures, including diet, physical activity, or external conditions can contribute to genotype\textendash environment interactions (G\texttimes E). Although high-dimensional environmental data are increasingly available and multiple exposures have been implicated with G\texttimes E at the same loci, multi-environment tests for G\texttimes E are not established. Here, we propose the structured linear mixed model (StructLMM), a computationally efficient method to identify and characterize loci that interact with one or more environments. After validating our model using simulations, we applied StructLMM to body mass index in the UK Biobank, where our model yields previously known and novel G\texttimes E signals. Finally, in an application to a large blood eQTL dataset, we demonstrate that StructLMM can be used to study interactions with hundreds of environmental variables.},
  keywords = {\_tablet,Functional genomics,Genetic association study,Statistics},
  file = {/home/wcasazza/Dropbox/Zotero/storage/7CEQV73J/Moore et al_2019_A linear mixed-model approach to study multivariate gene–environment.pdf}
}

