@article{kumarWholegenomeRiskPrediction2022,
  title = {Whole-Genome Risk Prediction of Common Diseases in Human Preimplantation Embryos},
  author = {Kumar, Akash and Im, Kate and Banjevic, Milena and Ng, Pauline C. and Tunstall, Tate and Garcia, Geronimo and Galhardo, Luisa and Sun, Jiayi and Schaedel, Oren N. and Levy, Brynn and Hongo, Donna and Kijacic, Dusan and Kiehl, Michelle and Tran, Nam D. and Klatsky, Peter C. and Rabinowitz, Matthew},
  year = {2022},
  month = mar,
  journal = {Nature Medicine},
  volume = {28},
  number = {3},
  pages = {513--516},
  publisher = {{Nature Publishing Group}},
  issn = {1546-170X},
  doi = {10.1038/s41591-022-01735-0},
  urldate = {2023-07-28},
  abstract = {Preimplantation genetic testing (PGT) of in-vitro-fertilized embryos has been proposed as a method to reduce transmission of common disease; however, more comprehensive embryo genetic assessment, combining the effects of common variants and rare variants, remains unavailable. Here, we used a combination of molecular and statistical techniques to reliably infer inherited genome sequence in 110 embryos and model susceptibility across 12 common conditions. We observed a genotype accuracy of 99.0\textendash 99.4\% at sites relevant to polygenic risk scoring in cases from day-5 embryo biopsies and 97.2\textendash 99.1\% in cases from day-3 embryo biopsies. Combining rare variants with polygenic risk score (PRS) magnifies predicted differences across sibling embryos. For example, in a couple with a pathogenic BRCA1 variant, we predicted a 15-fold difference in odds ratio (OR) across siblings when combining versus a 4.5-fold or 3-fold difference with BRCA1 or PRS alone. Our findings may inform the discussion of utility and implementation of genome-based PGT in clinical practice.},
  copyright = {2022 The Author(s)},
  langid = {english},
  keywords = {Diagnostic markers,Genomics,Predictive medicine,to-read},
  file = {/home/wcasazza/Dropbox/Zotero/storage/942JM3WQ/Kumar et al_2022_Whole-genome risk prediction of common diseases in human preimplantation embryos.pdf}
}

