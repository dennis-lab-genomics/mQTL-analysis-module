@article{tamayoLimitationsSimpleGene2016,
  title = {The Limitations of Simple Gene Set Enrichment Analysis Assuming Gene Independence.},
  author = {Tamayo, Pablo and Steinhardt, George and Liberzon, Arthur and Mesirov, Jill P},
  year = {2016},
  month = feb,
  journal = {Statistical methods in medical research},
  volume = {25},
