@article{yangGCTAToolGenomewide2011,
  title = {{{GCTA}}: {{A Tool}} for {{Genome-wide Complex Trait Analysis}}},
  shorttitle = {{{GCTA}}},
  author = {Yang, Jian and Lee, S. Hong and Goddard, Michael E. and Visscher, Peter M.},
