@article{youngGenomewideBrainDNA2019,
  title = {Genome-Wide Brain {{DNA}} Methylation Analysis Suggests Epigenetic Reprogramming in {{Parkinson}} Disease},
  author = {Young, Juan I. and Sivasankaran, Sathesh K. and Wang, Lily and Ali, Aleena and Mehta, Arpit and Davis, David A. and Dykxhoorn, Derek M. and Petito, Carol K. and Beecham, Gary W. and Martin, Eden R. and Mash, Deborah C. and {Pericak-Vance}, Margaret and Scott, William K. and Montine, Thomas J. and Vance, Jeffery M.},
  year = {2019},
