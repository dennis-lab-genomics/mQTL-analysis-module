@article{rutherfordNovelMutationsTARDBP2008,
  title = {Novel {{Mutations}} in {{TARDBP}} ({{TDP-43}}) in {{Patients}} with {{Familial Amyotrophic Lateral Sclerosis}}},
  author = {Rutherford, Nicola J. and Zhang, Yong-Jie and Baker, Matt and Gass, Jennifer M. and Finch, NiCole A. and Xu, Ya-Fei and Stewart, Heather and Kelley, Brendan J. and Kuntz, Karen and Crook, Richard J. P. and Sreedharan, Jemeen and Vance, Caroline and Sorenson, Eric and Lippa, Carol and Bigio, Eileen H. and Geschwind, Daniel H. and Knopman, David S. and Mitsumoto, Hiroshi and Petersen, Ronald C. and Cashman, Neil R. and Hutton, Mike and Shaw, Christopher E. and Boylan, Kevin B. and Boeve, Bradley and {Graff-Radford}, Neill R. and Wszolek, Zbigniew K. and Caselli, Richard J. and Dickson, Dennis W. and Mackenzie, Ian R. and Petrucelli, Leonard and Rademakers, Rosa},
  year = {2008},
  month = sep,
  journal = {PLOS Genetics},
  volume = {4},
  number = {9},
  pages = {e1000193},
  publisher = {{Public Library of Science}},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1000193},
  urldate = {2023-06-28},
  abstract = {The TAR DNA-binding protein 43 (TDP-43) has been identified as the major disease protein in amyotrophic lateral sclerosis (ALS) and frontotemporal lobar degeneration with ubiquitin inclusions (FTLD-U), defining a novel class of neurodegenerative conditions: the TDP-43 proteinopathies. The first pathogenic mutations in the gene encoding TDP-43 (TARDBP) were recently reported in familial and sporadic ALS patients, supporting a direct role for TDP-43 in neurodegeneration. In this study, we report the identification and functional analyses of two novel and one known mutation in TARDBP that we identified as a result of extensive mutation analyses in a cohort of 296 patients with variable neurodegenerative diseases associated with TDP-43 histopathology. Three different heterozygous missense mutations in exon 6 of TARDBP (p.M337V, p.N345K, and p.I383V) were identified in the analysis of 92 familial ALS patients (3.3\%), while no mutations were detected in 24 patients with sporadic ALS or 180 patients with other TDP-43\textendash positive neurodegenerative diseases. The presence of p.M337V, p.N345K, and p.I383V was excluded in 825 controls and 652 additional sporadic ALS patients. All three mutations affect highly conserved amino acid residues in the C-terminal part of TDP-43 known to be involved in protein-protein interactions. Biochemical analysis of TDP-43 in ALS patient cell lines revealed a substantial increase in caspase cleaved fragments, including the {$\sim$}25 kDa fragment, compared to control cell lines. Our findings support TARDBP mutations as a cause of ALS. Based on the specific C-terminal location of the mutations and the accumulation of a smaller C-terminal fragment, we speculate that TARDBP mutations may cause a toxic gain of function through novel protein interactions or intracellular accumulation of TDP-43 fragments leading to apoptosis.},
  langid = {english},
  keywords = {Alzheimer's disease,Amyotrophic lateral sclerosis,DNA-binding proteins,Heredity,Missense mutation,Mutation,Mutation detection,Neurodegenerative diseases},
  file = {/home/wcasazza/Dropbox/Zotero/storage/URCDL6X4/Rutherford et al_2008_Novel Mutations in TARDBP (TDP-43) in Patients with Familial Amyotrophic.pdf}
}

