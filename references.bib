@article{bhattacharyaBestPracticesMultiancestry2022,
  title = {Best Practices for Multi-Ancestry, Meta-Analytic Transcriptome-Wide Association Studies: {{Lessons}} from the {{Global Biobank Meta-analysis Initiative}}},
  shorttitle = {Best Practices for Multi-Ancestry, Meta-Analytic Transcriptome-Wide Association Studies},
  author = {Bhattacharya, Arjun and Hirbo, Jibril B. and Zhou, Dan and Zhou, Wei and Zheng, Jie and Kanai, Masahiro and Pasaniuc, Bogdan and Gamazon, Eric R. and Cox, Nancy J.},
