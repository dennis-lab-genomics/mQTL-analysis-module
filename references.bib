  author = {Schousboe, Karoline and Willemsen, Gonneke and Kyvik, Kirsten O. and Mortensen, Jakob and Boomsma, Dorret I. and Cornes, Belinda K. and Davis, Chayna J. and Fagnani, Corrado and Hjelmborg, Jacob and Kaprio, Jaakko and de Lange, Marlies and Luciano, Michelle and Martin, Nicholas G. and Pedersen, Nancy and Pietil{\"a}inen, Kirsi H. and Rissanen, Aila and Saarni, Suoma and S{\o}rensen, Thorkild I. A. and van Baal, G. Caroline M. and Harris, Jennifer R.},
  year = {2003},
