@article{blanco-gomezMissingHeritabilityComplex2016,
  title = {Missing Heritability of Complex Diseases: {{Enlightenment}} by Genetic Variants from Intermediate Phenotypes},
  shorttitle = {Missing Heritability of Complex Diseases},
  author = {{Blanco-G{\'o}mez}, Adri{\'a}n and {Castillo-Lluva}, Sonia and {del Mar S{\'a}ez-Freire}, Mar{\'i}a and {Hontecillas-Prieto}, Lourdes and Mao, Jian Hua and {Castellanos-Mart{\'i}n}, Andr{\'e}s and {P{\'e}rez-Losada}, Jesus},
