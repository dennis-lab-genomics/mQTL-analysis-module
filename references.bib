  langid = {english},
  file = {/home/wcasazza/Dropbox/Zotero/storage/HVQPU6NS/Grotzinger et al. - 2020 - Genetic Architecture of 11 Major Psychiatric Disor.pdf;/home/wcasazza/Dropbox/Zotero/storage/5VVLINZQ/2020.09.22.html}
}

@techreport{grotzingerGeneticArchitecture112020a,
  type = {Preprint},
  title = {Genetic {{Architecture}} of 11 {{Major Psychiatric Disorders}} at {{Biobehavioral}}, {{Functional Genomic}}, and {{Molecular Genetic Levels}} of {{Analysis}}},
  author = {Grotzinger, Andrew D. and Mallard, Travis T. and Akingbuwa, Wonuola A. and Ip, Hill F. and Adams, Mark J. and Lewis, Cathryn M. and McIntosh, Andrew M. and Grove, Jakob and Dalsgaard, S{\o}ren and Lesch, Klaus-Peter and Strom, Nora and Meier, Sandra M. and Mattheisen, Manuel and B{\o}rglum, Anders D. and Mors, Ole and Breen, Gerome and {iPSYCH, Tourette Syndrome and Obsessive Compulsive Disorder Working Group of the Psychiatric Genetics Consortium} and {Bipolar Disorder Working Group of the Psychiatric Genetics Consortium} and {Major Depressive Disorder Working Group of the Psychiatric Genetics Consortium} and {Schizophrenia Working Group of the Psychiatric Genetics Consortium} and Lee, Phil H. and Kendler, Kenneth S. and Smoller, Jordan W. and {Tucker-Drob}, Elliot M. and Nivard, Michel G.},
  year = {2020},
  month = sep,
  institution = {{Genetic and Genomic Medicine}},
  doi = {10.1101/2020.09.22.20196089},
  urldate = {2021-04-16},
  abstract = {Abstract           We systematically interrogate the joint genetic architecture of 11 major psychiatric disorders at biobehavioral, functional genomic, and molecular genetic levels of analysis. We identify four broad factors (Neurodevelopmental, Compulsive, Psychotic, and Internalizing) that underlie genetic correlations among the disorders, and test whether these factors adequately explain their genetic correlations with biobehavioral traits. We introduce Stratified Genomic Structural Equation Modelling, which we use to identify gene sets and genomic regions that disproportionately contribute to pleiotropy, including protein-truncating variant intolerant genes expressed in excitatory and GABAergic brain cells that are enriched for pleiotropy between disorders with psychotic features. Multivariate association analyses detect a total of 152 (20 novel) independent loci which act on the four factors, and identify nine loci that act heterogeneously across disorders within a factor. Despite moderate to high genetic correlations across all 11 disorders, we find very little utility of, or evidence for, a single dimension of genetic risk across psychiatric disorders.},
  langid = {english},
  file = {/home/wcasazza/Dropbox/Zotero/storage/Z75PRNX3/Grotzinger et al. - 2020 - Genetic Architecture of 11 Major Psychiatric Disor.pdf}
}

