}

@article{uffelmannEmergingMethodsResources2021,
  title = {Emerging {{Methods}} and {{Resources}} for {{Biological Interrogation}} of {{Neuropsychiatric Polygenic Signal}}},
  author = {Uffelmann, Emil and Posthuma, Danielle},
  year = {2021},
  month = jan,
  journal = {Biological Psychiatry},
  volume = {89},
  number = {1},
  pages = {41--53},
  issn = {00063223},
  doi = {10.1016/j.biopsych.2020.05.022},
  urldate = {2021-06-22},
  abstract = {Most neuropsychiatric disorders are highly polygenic, implicating hundreds to thousands of causal genetic variants that span much of the genome. This widespread polygenicity complicates biological understanding because no single variant can explain disease etiology. A strategy to advance biological insight is to seek convergent functions among the large set of variants and map them to a smaller set of disease-relevant genes and pathways. Accordingly, functional genomic resources that provide data on intermediate molecular phenotypes, such as gene-expression and methylation status, can be leveraged to functionally annotate variants and map them to genes. Such molecular quantitative trait locus mappings can be integrated with genome-wide association studies to make sense of the polygenic signal that underlies complex disease. Other resources that provide data on the 3-dimensional structure of chromatin and functional importance of specific genomic regions can be integrated similarly. In addition, mapped genes can then be tested for convergence in biological function, tissue, cell type, or developmental stage. In this review, we provide an overview of functional genomic resources and methods that can be used to interpret results from genome-wide association studies, and we discuss current challenges for biological understanding and future requirements to overcome them.},
  langid = {english},
  file = {/home/wcasazza/Dropbox/Zotero/storage/JP5ZFJ7W/Uffelmann and Posthuma - 2021 - Emerging Methods and Resources for Biological Inte.pdf;/home/wcasazza/Dropbox/Zotero/storage/MDPIYU5I/1-s2.0-S0006322320316280-mmc1.pdf}
}

@article{uhHowDealEarly2012,
  title = {How to Deal with the Early {{GWAS}} Data When Imputing and Combining Different Arrays Is Necessary},
  author = {Uh, Hae-Won and Deelen, Joris and Beekman, Marian and Helmer, Quinta and Rivadeneira, Fernando and Hottenga, Jouke-Jan and Boomsma, Dorret I. and Hofman, Albert and Uitterlinden, Andr{\'e} G. and Slagboom, P. E. and B{\"o}hringer, Stefan and {Houwing-Duistermaat}, Jeanine J.},
  year = {2012},
  month = may,
  journal = {European Journal of Human Genetics},
  volume = {20},
  number = {5},
  pages = {572--576},
  publisher = {{Nature Publishing Group}},
  issn = {1476-5438},
  doi = {10.1038/ejhg.2011.231},
  urldate = {2020-05-06},
  abstract = {Genotype imputation has become an essential tool in the analysis of genome-wide association scans. This technique allows investigators to test association at ungenotyped genetic markers, and to combine results across studies that rely on different genotyping platforms. In addition, imputation is used within long-running studies to reuse genotypes produced across generations of platforms. Typically, genotypes of controls are reused and cases are genotyped on more novel platforms yielding a case\textendash control study that is not matched for genotyping platforms. In this study, we scrutinize such a situation and validate GWAS results by actually retyping top-ranking SNPs with the Sequenom MassArray platform. We discuss the needed quality controls (QCs). In doing so, we report a considerable discrepancy between the results from imputed and retyped data when applying recommended QCs from the literature. These discrepancies appear to be caused by extrapolating differences between arrays by the process of imputation. To avoid false positive results, we recommend that more stringent QCs should be applied. We also advocate reporting the imputation quality measure (RT2) for the post-imputation QCs in publications.},
  copyright = {2012 The Author(s)},
  langid = {english},
  file = {/home/wcasazza/Dropbox/Zotero/storage/UMVZ7U35/Uh et al. - 2012 - How to deal with the early GWAS data when imputing.pdf;/home/wcasazza/Dropbox/Zotero/storage/M8THAWSB/ejhg2011231.html}
}

