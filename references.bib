@article{kachuriGeneExpressionAfrican2023,
  title = {Gene Expression in {{African Americans}}, {{Puerto Ricans}} and {{Mexican Americans}} Reveals Ancestry-Specific Patterns of Genetic Architecture},
  author = {Kachuri, Linda and Mak, Angel C. Y. and Hu, Donglei and Eng, Celeste and Huntsman, Scott and Elhawary, Jennifer R. and Gupta, Namrata and Gabriel, Stacey and Xiao, Shujie and Keys, Kevin L. and {Oni-Orisan}, Akinyemi and {Rodr{\'i}guez-Santana}, Jos{\'e} R. and LeNoir, Michael A. and Borrell, Luisa N. and Zaitlen, Noah A. and Williams, L. Keoki and Gignoux, Christopher R. and Burchard, Esteban Gonz{\'a}lez and Ziv, Elad},
  year = {2023},
